    inner_pipe_axial_face
    {
        type            zeroGradient;
    }
    inner_pipe_inner_face
    {
        type            zeroGradient;
    }
    strut_face
    {
        type            zeroGradient;
    }
    inner_pipe_outer_face
    {
        type            zeroGradient;
    }
